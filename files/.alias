alias ..='cd ..'

alias catala='aspell --lang=catala check'
alias corrige='aspell --lang=francais check'

alias d='du -s --block-size=1M * . | sort -n'
alias f='find . | grep'

alias rm='rm -i'

alias l='ls -G'
alias la='l -a'
alias ll='l -l'
alias lla='l -l -a'

alias gvim='mvim'

# Svn diff
alias sd='svn diff --diff-cmd=colordiff'
alias gsd='svnvimdiff -g'

alias scipy='ipython -p scipy'
alias remake='make clean;make'
alias v='ctags -R *.c *.cc *.cpp *.cxx;vim'


alias jhead-rename-with-date='jhead -nf%Y%m%d-%H%M%S'

